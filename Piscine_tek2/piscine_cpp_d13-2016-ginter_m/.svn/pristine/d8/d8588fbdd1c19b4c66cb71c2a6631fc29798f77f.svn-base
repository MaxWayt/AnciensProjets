#include "Buzz.h"

Buzz::Buzz(std::string const& name, std::string const& filename) :
    Toy(BUZZ, name, filename)
{}

Buzz::~Buzz()
{}

void Buzz::speak(std::string const& speech)
{
    std::cout << "BUZZ: ";
    Toy::speak(speech);
}
