#include "Toy.h"

Toy::Toy(std::string const& title, ObjectSubType subType) : Object(TYPE_TOY, subType), _title(title)
{
}

bool Toy::isTaken() const
{
    return _taken;
}

void Toy::setTaken(bool val)
{
    _taken = val;
}

const std::string& getTitle() const
{
    return _title;
}
